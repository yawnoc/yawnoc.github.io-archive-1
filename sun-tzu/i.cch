  \unit{haystack (秭)} &= 10^{12} \\
  \unit{soil (壤)} &= 10^{13} \\
  \unit{ditch (溝)} &= 10^{14} \\
  \unit{stream (澗)} &= 10^{15} \\
  \unit{right (正)} &= 10^{16} \\
  \unit{carry (載)} &= 10^{17}
